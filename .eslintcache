localhost:3001/selectedMovie\",{id:params.movieId},{\n       \n           headers: {\n   \n               'Content-Type' : 'application/json'\n   \n           }\n   \n       }).then((result)=>{\n           \n           if(result.status===200){\n           console.log(result.data);\n           setMovieDetails([...movieDetails,result.data]);\n           //console.log(movieDetails);\n        }}).catch(err=>{\n               console.log(err.response);\n           \n       });\n       await axios.post(\"http://localhost:3001/getMovieRating\",{id:params.movieId},{\n       \n           headers: {\n   \n               'Content-Type' : 'application/json'\n   \n           }\n   \n       }).then((result)=>{\n           \n           if(result.status===201){\n           console.log(result.data);\n           setCurrentValue(result.data);\n           //console.log(movieDetails);\n        }}).catch(err=>{\n               console.log(err.response);\n           \n       });\n    \n     }\n     const getLoggedUser=async ()=>{await axios.get(\"http://localhost:3007/login\").then((response) => {\n      if (response.data.loggedIn == true) {\n          console.log(response.data.loggedIn);\n          \n         \n        setLoginStatus(response.data.user[0].id);\n      }\n    });};\n     const Addfave= ()=>{\n         axios.post(\"http://localhost:3001/addFave\",{id_user:loginStatus,id_movie:params.movieId,},{\n       \n            headers: {\n    \n                'Content-Type' : 'application/json'\n    \n            }\n    \n        }).then((result)=>{\n            \n            if(result.status===201){\n            console.log(result.data);\n            \n            //console.log(movieDetails);\n         }}).catch(err=>{\n                console.log(err.response);\n            \n        });\n     \n     }\n     const handleClick = value => {\n        setCurrentValue(value)\n      }\n    \n      const handleMouseOver = newHoverValue => {\n        setHoverValue(newHoverValue)\n      };\n    \n      const handleMouseLeave = () => {\n        setHoverValue(undefined)\n      }\n     const submitRating= async () =>\n     {\n        await axios.post(\"http://localhost:3001/updateMovieRating\",{id:params.movieId,rating:currentValue},{\n       \n            headers: {\n    \n                'Content-Type' : 'application/json'\n    \n            }\n    \n        }).then((result)=>{\n            \n            if(result.status===200){\n            console.log(result.data);\n            setCurrentValue(result.data);\n            //console.log(movieDetails);\n         }}).catch(err=>{\n                console.log(err.response);\n            \n        });\n        \n     }\n     \n     return (\n        <StyledContainer>\n            {movieDetails?.map( (val, key) => {\n                \n                \n                return (\n                <div className=\"details\">\n                    <div className=\"big-img\">\n                        <img src={val.image} alt=\"\"/>\n                    </div>\n                    \n                    <div className=\"box\">\n                        <div className=\"row\">\n                            <div class=\"player\">\n                             <ReactPlayer url=\"https://www.youtube.com/watch?v=pzD9zGcUNrw\"/>\n                             </div>\n                        </div>\n              \n                       <p>{val.description}</p>\n           \n\n                     \n                        <div style={styles.stars}>\n                        {stars.map((_, index) => {\n                        return (\n                        <FaStar\n                          key={index}\n                          size={24}\n                          onClick={() => handleClick(index + 1)}\n                          onMouseOver={() => handleMouseOver(index + 1)}\n                          onMouseLeave={handleMouseLeave}\n                          color={(hoverValue || currentValue) > index ? colors.orange : colors.grey}\n                          style={{\n                         marginRight: 10,\n                        cursor: \"pointer\"\n              }}\n            />\n           \n          )\n         \n        })}\n         <StyledButtonWLinkR onClick={()=>submitRating()}>Rate</StyledButtonWLinkR>\n\n      </div>\n      <StyledButtonWLink>Buy ticket</StyledButtonWLink>\n      <StyledButtonWLink onClick={()=>Addfave()}>Add to favourites</StyledButtonWLink>\n\n                    </div>\n                </div>)\n                ;}\n        \n          \n            )}\n            </StyledContainer>\n    );\n    \n         \n    \n};\n\nexport default MovieDetails;","/Users/iasminapurcar/Documents/cinemadness/Cinemadness/src/components/Users/UnloggedUser.js",[],"/Users/iasminapurcar/Documents/cinemadness/Cinemadness/src/components/Movies/AddNewMovie.js",[],"/Users/iasminapurcar/Documents/cinemadness/Cinemadness/src/components/Movies/NewMovieForm.js",["93","94","95","96"],"/Users/iasminapurcar/Documents/cinemadness/Cinemadness/src/components/Movies/Movies.js",["97","98","99","100"],"/Users/iasminapurcar/Documents/cinemadness/Cinemadness/src/components/Layout/Footer.js",[],"/Users/iasminapurcar/Documents/cinemadness/Cinemadness/src/components/Movies/MovieItem.js",["101","102","103"],"import classes from './MovieItem.module.css';\nimport { Fragment, useEffect,useState } from 'react';\nimport axios from \"axios\";\nconst MovieItem = (props) => {\n   \n    return <Fragment>\n                <section className={classes.movie}>\n                    <img src={props.image} alt={props.name} />\n                    <div className={classes['movie-info']}>\n                        <h3>{props.name}</h3>\n                    \n                    </div>\n                </section>\n            </Fragment>\n}\n\nexport default MovieItem;","/Users/iasminapurcar/Documents/cinemadness/Cinemadness/src/components/Layout/Register.js",["104","105","106"],"import React from \"react\";\n